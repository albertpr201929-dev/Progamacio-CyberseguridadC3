Algoritmo NUMERO_PARES
    DEFINIR NumeroPar COMO ENTERO;
	
    ESCRIBIR "--- Números Pares entre 1 y 50 ---";
	

    PARA NumeroPar DESDE 2 HASTA 50 CON PASO 2 HACER
        
        ESCRIBIR NumeroPar;
        
    FinPara
	
    ESCRIBIR "-----------------------------------";
    ESCRIBIR "Fin de la lista.";





FinAlgoritmo
